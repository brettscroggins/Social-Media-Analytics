    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def get_topics(x):\n",
    "    pattern = \"\\(\\d+\\,\"\n",
    "    x = str(x)\n",
    "    matches = re.findall(pattern, x)\n",
    "    list_of_m = []\n",
    "    for m in matches:\n",
    "        m = m.lstrip(\"(\")\n",
    "        m = m.rstrip(\",\")\n",
    "        list_of_m.append(m)\n",
    "    return list_of_m\n",
    "        \n",
    "doc_topics_df['Topics'] = doc_topics_df['Doc Topics'].map(get_topics)"
   ]
  },
