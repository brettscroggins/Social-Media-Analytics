    "def new_columns(i, lst):\n",
    "    if i in lst:\n",
    "        return 1\n",
    "    else:\n",
    "        return 0\n",
    "\n",
    "for j in range(0,15):\n",
    "    doc_topics_df['Topic ' + str(j)] = doc_topics_df['Topics'].apply(lambda x: new_columns(str(j), x))\n",
    "\n",
    "doc_topics_df.head()"
   ]
