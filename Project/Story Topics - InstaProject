   "source": [
    "#### Get all likes in thousands"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def likes_num(x):\n",
    "    match = re.findall('k',x)\n",
    "    if len(match) == 0:\n",
    "        return int(float(x.rstrip('m'))*1000)\n",
    "    else: \n",
    "        return int(float(x.rstrip('k')))"
   ]
  },
