    "def clean(doc):\n",
    "    doc = re.sub(r'[^\\w\\s]*', '', doc) \n",
    "    doc = re.sub(r'[\\s]+', ' ', doc)\n",
    "    doc = doc.lower().strip()\n",
    "    return doc\n",
    "\n",
    "df['words'] = df['post'].map(lambda x: clean(x).split())\n",
    "clean_docs = df['words'].tolist()\n",
    "# clean_docs"
   ]
  },
