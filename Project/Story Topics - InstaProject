   "cell_type": "code",
   "execution_count": 10,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def comments_num(x):\n",
    "    if \",\" in x:\n",
    "        x = x.replace(',','')\n",
    "        return int(float(x))\n",
    "    elif \"k\" in x:\n",
    "        x = x.rstrip(\"k\")\n",
    "        return int(float(x)*1000)\n",
    "    else:\n",
    "        return int(x)"
   ]
  },
